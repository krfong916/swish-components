{
  "name": "Swish-Design",
  "title": "Swish Design",
  "description": "An enterprise-ready and accessible UI design library of React components",
  "version": "0.1.1",
  "license": "MIT",
  "keywords": [
    "swish",
    "components",
    "design",
    "react",
    "typescript",
    "frontend",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/swish-components"
  },
  "author": "Kyle Fong",
  "bugs": {
    "url": "https://github.com/swish-components/issues"
  },
  "homepage": "https://github.com/swish-components",
  "main": "build/lib/index.js",
  "scripts": {
    "start": "start-storybook --quiet -p 6006 -c .storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build": "rimraf dist && build-storybook -c .storybook -o dist",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watch --config ./jest.config.js --runInBand",
    "test:cover": "jest --config ./jest.config.js --coverage && cat ./coverage/lcov.info | node_modules/.bin/coveralls",
    "format": "prettier --write \"src/**/*.+(ts|tsx|js|css|scss|json)\"",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.+(ts|tsx|js|css|scss|json)": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@carbon/grid": "^10.11.0",
    "@material/layout-grid": "^6.0.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "auto": "^10.16.5",
    "babel-loader": "^8.0.6",
    "chromatic": "^5.6.2",
    "classnames": "^2.2.6",
    "coveralls": "^3.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsdoc": "^15.3.4",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-testing-library": "^3.10.1",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "husky": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-transform-css": "^2.1.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "rimraf": "^2.6.3",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
