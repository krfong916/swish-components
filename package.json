{
  "name": "Swish",
  "version": "1.0.0",
  "description": "Swish Components",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@carbon/grid": "^10.10.2",
    "@material/layout-grid": "^6.0.0",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^5.3.19",
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.2.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "coveralls": "^3.0.9",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsdoc": "^15.3.4",
    "eslint-plugin-json": "^1.4.0",
    "husky": "^4.2.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "start": "start-storybook",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watch --config ./jest.config.js",
    "test:cover": "jest --config ./jest.config.js --coverage && cat ./coverage/lcov.info | node_modules/.bin/coveralls",
    "format": "prettier --write \"src/**/*.+(js|css|json)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.+(js|css|json)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "plugins": [
      "prettier",
      "json"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true
    }
  },
  "dependencies": {}
}
