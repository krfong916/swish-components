{
  "name": "Swish",
  "version": "1.0.0",
  "description": "Swish Components",
  "main": "build/lib/index.js",
  "license": "MIT",
  "devDependencies": {
    "@carbon/grid": "^10.11.0",
    "@material/layout-grid": "^6.0.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^5.10.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.2.3",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "coveralls": "^3.0.9",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsdoc": "^15.3.4",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build": "build-storybook",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watch --config ./jest.config.js",
    "test:cover": "jest --config ./jest.config.js --coverage && cat ./coverage/lcov.info | node_modules/.bin/coveralls",
    "format": "prettier --write \"src/**/*.+(js|css|json)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.+(js|json)": [
      "prettier --write",
      "git add"
    ]
  }
}
